<?xml version="1.0" encoding="utf-8" ?>
<!--
    For WPF Localization.
    Designer: functionghw<functionghw@hotmail.com>
    
    Example:    
        <LocalizationCollection CultureName="en-US">
            <StringItem>
                <MsgId>TheId</MsgId>
                <MsgStr>TheMessage</MsgStr>
            </StringItem>
        </LocalizationCollection>
-->
<LocalizationCollection CultureName="en-US">
    <StringItem>
        <MsgId>test</MsgId>
        <MsgStr>Test</MsgStr>
    </StringItem>
    <StringItem>
        <MsgId>name</MsgId>
        <MsgStr>Name</MsgStr>
    </StringItem>
    <StringItem>
        <MsgId>InputSomething</MsgId>
        <MsgStr>{0} can not be empty, please input something!</MsgStr>
    </StringItem>
</LocalizationCollection>
