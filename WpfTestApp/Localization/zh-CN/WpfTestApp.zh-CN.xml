<LocalizationCollection CultureName="zh-CN">
    <StringItem>
        <MsgId>test</MsgId>
        <MsgStr>测试</MsgStr>
    </StringItem>
    <StringItem>
        <MsgId>name</MsgId>
        <MsgStr>姓名</MsgStr>
    </StringItem>
    <StringItem>
        <MsgId>InputSomething</MsgId>
        <MsgStr>{0}是必填项，输入点东西吧！</MsgStr>
    </StringItem>
</LocalizationCollection>
