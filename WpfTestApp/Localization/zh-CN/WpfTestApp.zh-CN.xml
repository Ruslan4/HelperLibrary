<?xml version="1.0" encoding="utf-8" ?>
<!--
    For WPF Localization.
    Designer: functionghw<functionghw@hotmail.com>
    
    Example:    
            Using attributes:
                <StringItem Key="thekey" Value="the value"/>
                
            Using elements (either Key or Value, or both):
                <StringItem Key="using attribute">
                    <Value>using element</Value>
                </StringItem>>
                
            For special char like "&", "<", ">", '"' etc:
                <StringItem>
                    <Key>the key</Key>
                    <Value>if (num <![CDATA[<>]]> 0 <![CDATA[&&]]> val == "value")</Value>
                </StringItem>>
-->
<LocalizedStrings>
    <StringItem>
        <Key>test</Key>
        <Value>测试</Value>
    </StringItem>
    <StringItem>
        <Key>name</Key>
        <Value>姓名</Value>
    </StringItem>
    <StringItem>
        <Key>InputSomething</Key>
        <Value>{0}是必填项，输入点东西吧！</Value>
    </StringItem>
</LocalizedStrings>
